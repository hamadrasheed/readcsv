<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>All Data</title>
    <!-- Bootstrap CDN starts -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
    <!-- Bootstrap CDN ends -->
    <link href='https://fonts.googleapis.com/css?family=Roboto:400,100,300,700' rel='stylesheet' type='text/css'>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css"
        integrity="sha512-1ycn6IcaQQ40/MKBW2W4Rhis/DbILU74C1vSrLJxCq57o941Ym01SwNsOMqvEBFlcgUa6xLiPY/NS5R+E6ztJQ=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link rel="icon" type="image/png" href="images/wallet.png" />
    <link rel="stylesheet" href="css/expenses.css">
</head>

<body>
    <%- include('partials/header') %>
        <a class="fixedButton" href="/dashboard">
            <div class="roundedFixedBtn"><i class="fa fa-plus"></i></div>
        </a>
        <section class="ftco-section">
            <div class="container">
                <div class="row">
                    <% if(data?.author) { %>
                    <div class=" col-md-12">
                        <div class="flexer">
                            <h3 id="headerp">Author:
                            </h3>
                        </div>
                        <div class="table-wrap" style="border-radius: 15px;">
                            <table class="table" id="styledTable">
                                <thead class="thead-primary">
                                    <tr>
                                        <% for (const keys in data.author[0] || {}) { %>
                                            <th><%= keys %> </th>
                                        <% } %>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.author.forEach(e1=>{ %>
                                        <tr class="alert">
                                            <td>
                                                <div class="email">
                                                    <span>
                                                        <%=e1.email%>
                                                    </span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span>
                                                        <%=e1.firstname%>
                                                    </span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span><%=e1.lastname%></span>
                                                </div>
                                            </td>

                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <% } %>
                    <% if(data?.books) { %>
                    <div class=" col-md-12">
                        <div class="flexer">
                            <h3 id="headerp">Books:
                            </h3>
                        </div>
                        <div class="table-wrap" style="border-radius: 15px;">
                            <table class="table" id="styledTable">
                                <thead class="thead-primary">
                                    <tr>
                                        <% for (const keys in data.books[0] || {}) { %>
                                            <th><%= keys %> </th>
                                        <% } %>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.books.forEach(e1=>{ %>
                                        <tr class="alert">
                                            <td>
                                                <div class="email">
                                                    <span>
                                                        <%=e1.title%>
                                                    </span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span>
                                                        <%=e1.isbn%>
                                                    </span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span><%=e1.authors%></span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span><%=e1.description%></span>
                                                </div>
                                            </td>

                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <% } %>
                    <% if(data?.magzines) { %>
                    <div class=" col-md-12">
                        <div class="flexer">
                            <h3 id="headerp">Magzines:
                            </h3>
                        </div>
                        <div class="table-wrap" style="border-radius: 15px;">
                            <table class="table" id="styledTable">
                                <thead class="thead-primary">
                                    <tr>
                                        <% for (const keys in data.magzines[0] || {}) { %>
                                            <th><%= keys %> </th>
                                        <% } %>
                                        <th>&nbsp;</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% data.magzines.forEach(e1=>{ %>
                                        <tr class="alert">
                                            <td>
                                                <div class="email">
                                                    <span>
                                                        <%=e1.title%>
                                                    </span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span>
                                                        <%=e1.isbn%>
                                                    </span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span><%=e1.authors%></span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="email">
                                                    <span><%=e1.publishedAt%></span>
                                                </div>
                                            </td>

                                        </tr>
                                    <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <% } %>
                </div>
            </div>
        </section>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ"
            crossorigin="anonymous"></script>

        <!-- For jsPDF -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js"
            integrity="sha512-GsLlZN/3F2ErC5ifS5QtgpiJtWd43JWSuIgh7mbzZ8zBps+dvLusV+eNQATqgA/HdeKFVgA5v3S/cIrLF7QnIg=="
            crossorigin="anonymous" referrerpolicy="no-referrer"></script>

        <script src="js/generate.js"></script>
</body>

</html>